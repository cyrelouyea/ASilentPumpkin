; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="ASilentPumpkin"
run/main_scene="res://Scenes/Spatial.tscn"
config/icon="res://Textures/icon.png"

[autoload]

GlobalData="*res://Scripts/GlobalData.gd"
Audio="*res://Scenes/Audio.tscn"

[display]

window/size/width=640
window/size/height=280
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

fulls=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.45098, 0.45098, 0.45098, 1 )
environment/default_environment="res://default_env.tres"
